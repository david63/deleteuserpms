	"description": "Adds an ACP option to allow the deleting of a user's PM data.",
	"version": "3.3.0-RC1",
	"time": "2022-04-11",
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Extension developer"
		}],
	"require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[d63] Delete user PMs",
